<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.seguirapp.modelHibernate.EquipamentoModel" table="equipamento" schema="DBIFS">
        <id name="idEquipamento">
            <column name="idEquipamento" sql-type="int(11)"/>
        </id>
        <property name="marca">
            <column name="marca" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="idCoordenada">
            <column name="idCoordenada" sql-type="int(11)"/>
        </property>
        <property name="identificador">
            <column name="identificador" sql-type="varchar(10)" length="10"/>
        </property>
        <many-to-one name="coordenadaByIdCoordenada" class="com.seguirapp.modelHibernate.CoordenadaModel">
            <column name="idCoordenada"/>
        </many-to-one>
        <set name="servicosByIdEquipamento" inverse="true">
            <key>
                <column name="idEquipamento"/>
            </key>
            <one-to-many not-found="ignore" class="com.seguirapp.modelHibernate.ServicoModel"/>
        </set>
    </class>
</hibernate-mapping>