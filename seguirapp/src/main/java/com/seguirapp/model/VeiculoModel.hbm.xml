<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.seguirapp.model.VeiculoModel" table="veiculo" schema="DBIFS">
        <id name="idVeiculo">
            <column name="idVeiculo" sql-type="int(11)"/>
        </id>
        <property name="placa">
            <column name="placa" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="chassi">
            <column name="chassi" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="ano">
            <column name="ano" sql-type="date"/>
        </property>
        <property name="cor">
            <column name="cor" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="marca">
            <column name="marca" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="modelo">
            <column name="modelo" sql-type="varchar(45)" length="45"/>
        </property>
        <set name="servicosByIdVeiculo" inverse="true">
            <key>
                <column name="idVeiculo"/>
            </key>
            <one-to-many not-found="ignore" class="com.seguirapp.model.ServicoModel"/>
        </set>
    </class>
</hibernate-mapping>